<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<DialogPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="reminder.ChangeFinishedController"
            prefHeight="100.0" prefWidth="450.0">

    <content>
        <GridPane alignment="CENTER" hgap="10" vgap="10">
            <Label text="The mold change is finished at : " GridPane.rowIndex="0" GridPane.columnIndex="0"/>
            <TextField fx:id="hour" GridPane.rowIndex="0" GridPane.columnIndex="1" prefColumnCount="5"/>
            <Label text=" h : " GridPane.rowIndex="0" GridPane.columnIndex="2"/>
            <TextField fx:id="minutes" GridPane.rowIndex="0" GridPane.columnIndex="3" prefColumnCount="5"/>
            <Label text="m" GridPane.rowIndex="0" GridPane.columnIndex="4"/>
        </GridPane>
    </content>
</DialogPane>
